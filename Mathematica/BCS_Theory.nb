Cell[CellGroupData[{

Cell["Let\[CloseCurlyQuote]s solve the self-consistency equations \
numerically:", "Subsection",
 CellChangeTimes->{{3.819278643543045*^9, 
  3.819278653252062*^9}},ExpressionUUID->"bfd191f8-3175-4efd-b67a-\
8206d0493c24"],

Cell[TextData[{
 "Now, let\[CloseCurlyQuote]s solve the self-consistency equations \
numerically: \nHere both T and \[CapitalDelta] are in units of the Debye \
energy; g\[Rho]sh is a list of values of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Rho]", "F"], 
    RowBox[{"g", "/", "2"}]}], TraditionalForm]],ExpressionUUID->
  "274fb020-5eba-4ea2-90f9-a09899c0d6ad"],
 " plotted.\nThe gray dots are the corresponding analytic results for ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", "c"], TraditionalForm]],ExpressionUUID->
  "0cbf0af4-b8e8-4a66-a767-8deb024f51ea"],
 " and \[CapitalDelta](T=0). Clearly, we obtain very good agreement."
}], "Text",
 CellChangeTimes->{{3.819275261940372*^9, 3.819275264483665*^9}, {
  3.819275300618369*^9, 3.819275344978698*^9}, {3.819277468501604*^9, 
  3.819277490699712*^9}, {3.8192776074662733`*^9, 3.8192776716716537`*^9}, {
  3.819994514392191*^9, 
  3.819994515448783*^9}},ExpressionUUID->"9b6d5285-7804-4bab-ae84-\
7934a3ba6f91"],

Cell[BoxData[
 RowBox[{
  RowBox[{"KeyInteg", "[", 
   RowBox[{"\[CapitalDelta]_", ",", "T_"}], "]"}], ":=", 
  RowBox[{"NIntegrate", "[", 
   RowBox[{
    FractionBox[
     RowBox[{"Tanh", "[", 
      FractionBox[
       SqrtBox[
        RowBox[{
         SuperscriptBox["x", "2"], "+", 
         SuperscriptBox["\[CapitalDelta]", "2"]}]], 
       RowBox[{"2", " ", "T"}]], "]"}], 
     SqrtBox[
      RowBox[{
       SuperscriptBox["x", "2"], "+", 
       SuperscriptBox["\[CapitalDelta]", "2"]}]]], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.819275315684087*^9, 3.819275348490656*^9}, {
  3.8192756254127083`*^9, 3.8192756907906322`*^9}},
 CellLabel->"In[2]:=",ExpressionUUID->"4311e152-e63c-4645-9cac-323e71cbf503"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"g\[Rho]sh", "=", 
    RowBox[{"{", 
     RowBox[{"0.3", ",", "0.27", ",", "0.24", ",", "0.21", ",", "0.18"}], 
     "}"}]}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalDelta]s", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"1", "/", 
        RowBox[{"Sinh", "[", 
         RowBox[{"1", "/", "g\[Rho]"}], "]"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"g\[Rho]", ",", "g\[Rho]sh"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Tcs", "=", 
